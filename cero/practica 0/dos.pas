program uno;

var
   num1, num2, suma:integer;
begin
   writeln('Ingrese un numero:');
   readln(num1);
   writeln('Ingrese otro numero:');
   readln(num2);
   suma:=num1+num2;
   writeln('Los numeros ingresados son: ', num2,' y ', num1);
   writeln('La suma de ambos es: ', suma);
   writeln('La suma de ambos es: ', num1+num2);
   readln;
end.
